            <string>/// play other night ambiance sounds
if (start) {
    bird_timer = random(100)+50;
    bird_count = 0;
    crow_timer = random(30)+30;
    crow_count = floor(random(3)+1);
    cricket_timer = random(30)+10;
    start = false;
}

bird_timer--;
if (bird_timer &lt;= 0) {
    bird_timer = random(300)+50;
    bird_count++;
    if (bird_count &gt;= 2) {
        bird_timer += 400;
        bird_count = 0;
    }
    if (random(1) &lt; 0.5)
        audio_play_sound(snd_bird1_8, 2, false);
    else
        audio_play_sound(snd_bird2_8, 2, false);
}

crow_timer--;
if (crow_timer &lt;= 0) {
    crow_timer = random(80)+30;
    crow_count--;
    if (crow_count &lt;= 0) {
        crow_timer += 300;
        crow_count = floor(random(3)+1);
    }
    audio_play_sound(snd_crow8, 2, false);
}

cricket_timer--;
if (cricket_timer &lt;= 0) {
    cricket_timer = random(50)+20;
    var rnd = random(1)
    if (rnd &lt; 0.33)
        audio_play_sound(snd_cricket1_8, 1, false);
    else if (rnd &lt; 0.66)
        audio_play_sound(snd_cricket2_8, 1, false);
    else
        audio_play_sound(snd_cricket3_8, 1, false);
}
